<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Searchsongs</servlet-name>
    <servlet-class>iOS_Client.servlets.Searchsongs</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PlayandPause</servlet-name>
    <servlet-class>iOS_Client.servlets.PlayandPause</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AddToPlayList</servlet-name>
    <servlet-class>iOS_Client.servlets.AddToPlayList</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>Searchsongs</servlet-name>
    <url-pattern>/servlet/Searchsongs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PlayandPause</servlet-name>
    <url-pattern>/servlet/PlayandPause</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddToPlayList</servlet-name>
    <url-pattern>/servlet/AddToPlayList</url-pattern>
  </servlet-mapping>

</web-app>